{
  "name": "imagemin-loader",
  "version": "0.0.0-development",
  "description": "Imagemin loader module for webpack",
  "author": {
    "name": "Rafael Milewski",
    "email": "rafael.milewski@gmail.com",
    "url": "https://github.com/milewski"
  },
  "license": "MIT",
  "main": "source/loader.js",
  "files": [
    "source/*.js",
    "schema/*.json"
  ],
  "homepage": "https://github.com/milewski/imagemin-loader",
  "keywords": [
    "webpack",
    "imagemin",
    "loader",
    "webpack-loader",
    "image-loader",
    "optimize",
    "minify",
    "image"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/milewski/imagemin-loader"
  },
  "bugs": {
    "url": "https://github.com/milewski/imagemin-loader/issues"
  },
  "scripts": {
    "commit": "git-cz",
    "pretest": "npm run build",
    "test": "mocha -u tdd --no-timeouts",
    "build": "tsc",
    "prepublish": "npm run build && npm test",
    "prettier": "prettier \"*/**/*.ts\" --config .prettierrc --write && git add .",
    "semantic-release": "semantic-release"
  },
  "pre-commit": [
    "prettier",
    "test"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "ajv": "^6.4.0",
    "global": "^4.3.2",
    "imagemin": "^6.0.0",
    "loader-utils": "^1.1.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.0.0",
    "@types/node": "^10.0.3",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "expect.js": "^0.3.1",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-pngquant": "^6.0.0",
    "mocha": "^5.0.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.11.1",
    "semantic-release": "^15.13.1",
    "typescript": "^2.7.2"
  }
}
